    {
      "path": "dist/client.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/qrcode": "^1.4.0",
    "@types/ws": "^7.4.0",
    "husky": "^5.1.3",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "jsonrpc-lite": "^2.2.0",
    "qrcode": "^1.4.4",
    "ws": "^7.4.4"
